// OOPs Example: Car class
class Car {
    // Attributes
    String model;
    String color;

    // Constructor
    Car(String model, String color) {
        this.model = model;
        this.color = color;
    }

    // Method to start the car
    void start() {
        System.out.println(model + " (" + color + ") is starting...");
    }

    // Method to stop the car
    void stop() {
        System.out.println(model + " (" + color + ") has stopped.");
    }
}

// Main class
public class Main {
    public static void main(String[] args) {
        // Create objects
        Car car1 = new Car("BMW X5", "Black");
        Car car2 = new Car("Honda City", "Red");

        // Call methods
        car1.start();
        car1.stop();

        car2.start();
        car2.stop();
    }
}
